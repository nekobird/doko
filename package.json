{
  "name": "@nekobird/doko",
  "version": "0.0.3",
  "private": false,
  "description": "A really cool DOM utility library.",
  "homepage": "https://github.com/nekobird/doco#readme",
  "bugs": {
    "url": "https://github.com/nekobird/doco/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekobird/doco.git"
  },
  "license": "MIT",
  "author": "Andrew Prasetya",
  "sideEffects": false,
  "main": "build/commonjs/doko.js",
  "module": "build/esm/doko.js",
  "jsnext:main": "build/esm/doko.js",
  "types": "build/types/lib/doko.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly --outDir build/types/; rollup -c rollup.config.js",
    "build:docs": "typedoc --plugin typedoc-plugin-markdown --out docs lib",
    "clean": "rimraf build",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "test": "jest --watch"
  },
  "dependencies": {
    "@nekobird/piko": "0.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^26.1.0",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
