    "url": "git+https://github.com/nekobird/doco.git"
  },
  "license": "MIT",
  "author": "Andrew Prasetya",
  "files": [
    "build/**/*"
  ],
  "sideEffects": false,
  "main": "build/commonjs/doko.js",
